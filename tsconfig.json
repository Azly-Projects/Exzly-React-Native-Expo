{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@config": ["config"],
      "@components": ["src/components"],
      "@contexts": ["src/contexts"],
      "@interfaces": ["src/interfaces"],
      "@navigation": ["src/navigation"],
      "@screens/*": ["src/screens/*"],
      "@services": ["src/services"],
      "@styles": ["src/styles"]
    },
    "esModuleInterop": true
  },
  "extends": "expo/tsconfig.base"
}
